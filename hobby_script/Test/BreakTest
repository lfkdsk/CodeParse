lfkdsk = 0;
for(i = 1;i < 10;i = i + 1){
    if(i == 5){
        lfkdsk = 5;
        break;
    }
}

lfkdsk;

lfkdsk = "  lfkdsk";
for(i = 0; i < stringLength(lfkdsk); i = i + 1){
    peek = charAtPoint(lfkdsk, i);

    if(peek == " " || peek == "\\t"){

    }

    if (peek == "\\n"){
        // lineNumber = lineNumber + 1;
    } else {
        break;
    }

    logInfo("peek: " + peek);
}